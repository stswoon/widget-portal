    "portal:start-next": "next start -p 3100",
    "prettier:fix": "prettier . --write",
    "lint": "next lint",
    "copy-assets": "node buildHelper.mjs --copyStandaloneAssets"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
