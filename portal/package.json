{
  "name": "portal",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "portal:serve": "next dev --turbopack -p 3400",
    "portal:build": "npm run lint && npm run portal:build-standalone",
    "..portal:start": "npm run portal:start-standalone",
    "portal:start": "node server.js",
    "-----": "echo 0",
    "portal:build-standalone": "cross-env NEXT_DISABLE_ESLINT=1 next build && npm run copy-assets",
    "portal:start-standalone": "cross-env PORT=3400 node .next/standalone/server.js",
    "portal:build-next": "cross-env NEXT_DISABLE_ESLINT=1 next build",
    "portal:start-next": "next start -p 3400",
    "prettier:fix": "prettier . --write",
    "lint": "eslint .",
    "copy-assets": "node buildHelper.mjs --copyStandaloneAssets"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.7",
    "@mui/material-nextjs": "^7.3.7",
    "@mui/icons-material": "^7.3.7",
    "next": "16.1.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "marked": "^17.0.1",
    "path-to-regexp": "^8.3.0",
    "nanoid": "^5.1.6",
    "url-join": "^5.0.0",
    "swr": "^2.3.8"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "@types/node": "^25",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "3.8.1",
    "typescript": "^5"
  },
  "overrides": {
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3"
  }
}
